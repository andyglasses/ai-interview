<Application x:Class="AI.Interview.App.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AI.Interview.App"
             StartupUri="MainWindow.xaml">
  <Application.Resources>
    <BooleanToVisibilityConverter x:Key="BoolToVisibilityConverter" />
    <local:UnitConverter x:Key="UnitConverter" />
    <local:UnitNameConverter x:Key="UnitNameConverter" />
    <local:InvertedBooleanToVisibilityConverter x:Key="InvertedBoolToVisibilityConverter" />
  </Application.Resources>
</Application>
